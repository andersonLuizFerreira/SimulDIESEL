# Normalize line endings in repo
* text=auto

# Text formats
*.md text eol=lf
*.txt text eol=lf
*.ini text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.json text eol=lf
*.xml text eol=lf
*.h   text eol=lf
*.hpp text eol=lf
*.c   text eol=lf
*.cpp text eol=lf
*.ino text eol=lf

# Windows-specific
*.sln  text eol=crlf
*.csproj text eol=crlf# gitattributes 
